package br.unipar.pacman;

public class Ghost extends Character{

    private GhostType type; // Tipo de fantasma

    public Ghost(GhostType type, int x, int y, Direction direction) {
        this.type = type;
        this.x = x;
        this.y = y;
        this.direction = direction;
    }

    // Métodos específicos dos fantasmas
}
